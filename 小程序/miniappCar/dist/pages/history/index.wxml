<block>
    <view class="history-index-wrap">
        <view class="index-top">
            <view class="top-wrap" bindtap="handleCarChoice">
                <view class="left">
                    <view class="title">选择车辆</view>
                </view>
                <view class="arrow-wrap">
                    <text class="name">{{choiceCar.carName}}</text>
                    <image class="image" src="{{Arrow}}" />
                </view>
            </view>
        </view>
        <view class="index-center">
            <block wx:if="{{list.length > 0}}">
                <view class="title">历史轨迹</view>
            </block>
            <view class="lists">
                <scorll-history __triggerObserer="{{ _triggerObserer }}"></scorll-history>
            </view>
        </view>
    </view>
</block>