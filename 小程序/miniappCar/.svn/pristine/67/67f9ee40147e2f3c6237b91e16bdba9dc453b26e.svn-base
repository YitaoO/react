<block>
    <block wx:if="{{!!isShow}}">
        <cover-view class="marker-info" __triggerObserer="{{ _triggerObserer }}">
            <cover-view class="center" __triggerObserer="{{ _triggerObserer }}">
                <cover-view class="item" __triggerObserer="{{ _triggerObserer }}">
                    <cover-view class="left" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">车牌号</cover-view>
                        <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">{{info.carNumber}}</cover-view>
                    </cover-view>
                    <cover-view class="right" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">类型</cover-view>
                        <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">{{info.carModel}}</cover-view>
                    </cover-view>
                </cover-view>
                <cover-view class="item" __triggerObserer="{{ _triggerObserer }}">
                    <cover-view class="left" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">速度</cover-view>
                        <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">{{info.speed}} km/h</cover-view>
                    </cover-view>
                    <cover-view class="right" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">熄火</cover-view>
                        <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">{{!!info.acc ? "否" : "是"}}</cover-view>
                    </cover-view>
                </cover-view>
                <cover-view class="item" __triggerObserer="{{ _triggerObserer }}">
                    <cover-view class="left" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">司机</cover-view>
                        <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">{{!!info.driverName ? info.driverName : "无"}}</cover-view>
                    </cover-view>
                    <cover-view class="right" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">电话</cover-view>
                        <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">{{!!info.driverTel ? info.driverTel : "无"}}</cover-view>
                    </cover-view>
                </cover-view>
                <cover-view class="item" __triggerObserer="{{ _triggerObserer }}">
                    <cover-view class="left" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">定位时间</cover-view>
                        <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">{{info.adddate}}</cover-view>
                    </cover-view>
                </cover-view>
            </cover-view>
            <cover-view class="footer" __triggerObserer="{{ _triggerObserer }}" bindonclick="handleToDetail"
            __fn_onClick="{{true}}">
                <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">历史轨迹</cover-view>
                <cover-view class="image" __triggerObserer="{{ _triggerObserer }}"></cover-view>
            </cover-view>
        </cover-view>
    </block>
</block>