<block>
    <view class="history-index-wrap">
        <view class="index-top">
            <view class="top-wrap">
                <view class="left">
                    <view class="title">{{choiceCar.carName}}</view>
                    <view class="value">{{choiceCar.carNumber}}</view>
                </view>
                <view class="right" bindtap="handleCarChoice">选择车辆</view>
            </view>
        </view>
        <view class="index-center">
            <view class="title">历史轨迹</view>
            <view class="lists">
                <scorll-history __triggerObserer="{{ _triggerObserer }}"></scorll-history>
            </view>
        </view>
    </view>
</block>