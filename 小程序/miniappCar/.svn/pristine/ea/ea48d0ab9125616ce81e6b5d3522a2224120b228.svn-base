.map_container {
  height: 100%;
  width: 100%;
  .reload-btn {
    position: fixed;
    bottom: 100rpx;
    right: 10rpx;
    height: 132rpx;
    width: 132rpx;
    display: inline-block;
  }
  .header-btn {
    position: fixed;
    top: 20rpx;
    left: 20rpx;
    height: 150rpx;
    width: 150rpx;

    display: inline-block;
  }
  .mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    position: fixed;
    top: 0;
    left: 0;
  }
}
.myMap {
  height: 10px;
}
