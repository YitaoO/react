<block>
    <view class="search-container">
        <view class="item">
            <view class="left">
                <view class="title">已选车辆</view>
                <view class="value">{{!!choiceCar.carNumber ? choiceCar.carNumber : "所有车辆"}}</view>
            </view>
            <view class="right" bindtap="handleCarChoice">
                <text class="title">选择车辆</text>
                <image class="image" src="{{Arrow}}" />
            </view>
        </view>
        <view class="item">
            <view class="left">
                <view class="title">台班规则</view>
                <view class="value">8小时/班</view>
            </view>
            <view class="right" bindtap="handleCarChoice">
                <text class="title">选择规则</text>
                <image class="image" src="{{Arrow}}" />
            </view>
        </view>
        <view class="item">
            <view class="left">
                <view class="title">日期范围</view>
                <view class="value">2018年9月12日</view>
            </view>
            <view class="right" bindtap="handleCarChoice">
                <text class="title">选择日期</text>
                <image class="image" src="{{Arrow}}" />
            </view>
        </view>
        <view class="btn-search">查询</view>
    </view>
</block>