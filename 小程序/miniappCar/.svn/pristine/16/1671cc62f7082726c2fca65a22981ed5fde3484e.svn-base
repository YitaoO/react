<block>
    <view class="index-map-container">
        <map id="myMap" longitude="{{longitude}}" latitude="{{latitude}}" include-points="{{carStateList}}"
        markers="{{carStateList}}" bindmarkertap="handleMakertap" bindtap="handleMap"
        style="width: 100%; height: 100%;">
            <marker-info-window isShow="{{showMarkerInfo}}" __triggerObserer="{{ _triggerObserer }}"
            info="{{markerInfo}}"></marker-info-window>
            <cover-image class="reload-btn" src="{{reloadIcon}}"
            bindtap="handleReloadBtn"></cover-image>
            <cover-image class="header-btn" src="{{headerIcon}}" bindtap="handleMenu"></cover-image>
            <sider-menu __triggerObserer="{{ _triggerObserer }}"></sider-menu>
        </map>
    </view>
</block>