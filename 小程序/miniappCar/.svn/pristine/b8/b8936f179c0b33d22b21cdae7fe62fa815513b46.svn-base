.index-wrap,
.map-wrap {
  height: 100%;
}
.index-wrap {
  .header-btn {
    position: fixed;
    top: 20rpx;
    left: 20rpx;
    height: 150rpx;
    width: 150rpx;
    background: url("../../images/index_header_icon.png") no-repeat;
    background-size: 100% 100%;
    display: inline-block;
  }
  .mask {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    position: absolute;
    top: 0;
    left: 0;
    z-index: 10;
  }
}
