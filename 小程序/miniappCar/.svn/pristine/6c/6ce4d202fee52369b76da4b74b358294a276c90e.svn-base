.res-wrap {
  // height: 400rpx;
  position: fixed;
  top: 150rpx;
  bottom: 0;
  left: 50rpx;
  right: 50rpx;
  //   background: yellow;
  .wrap-center {
    .item {
      width: 100%;
      height: 80rpx;
      line-height: 80rpx;
      padding-left: 20rpx;
      background: rgba($color: #000000, $alpha: 0.5);
      border-radius: 40rpx;
      margin-bottom: 50rpx;
      box-sizing: border-box;

      .icon-wrap {
        width: 50rpx;
        height: 50rpx;
        position: relative;
        text-align: center;
        .icon {
          position: absolute;
          top: 15rpx;
          left: 0;
          right: 0;
          width: 45rpx;
          height: 45rpx;
          margin: auto;
          &.phone {
            background: url("../../images/reg_phone_icon.png") no-repeat;
            background-size: 100% 100%;
          }
          &.id {
            background: url("../../images/reg_id_icon.png") no-repeat;
            background-size: 100% 100%;
          }
        }
      }
      .input-wrap {
        font-size: 30rpx;
        height: 100%;
        padding-left: 10rpx;

        .input {
          height: 100%;
        }
      }
    }
  }
  .btn-wrap {
    margin-top: 50rpx;
    width: 100%;
    background: yellow;
  }
}
