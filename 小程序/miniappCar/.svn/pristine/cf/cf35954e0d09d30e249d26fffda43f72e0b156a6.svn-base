.map_container {
  height: 100%;
  width: 100%;
  .reload-btn {
    position: fixed;
    bottom: 100rpx;
    right: 10rpx;
    height: 132rpx;
    width: 132rpx;
    background: url("../../images/index_map_reload_icon.png") no-repeat;
    background-size: 100% 100%;
    display: inline-block;
  }
  .marker-info {
    width: 400rpx;
    height: 400rpx;
    left: 0;
    position: fixed;
    right: 0;
    // top: 0;
    bottom: 0;
    margin: auto;
    border-radius: 20rpx;
    padding: 30rpx;
    background: #fff;
    .center {
      .item {
        display: flex;
        padding-bottom: 25rpx;
      }
      .left,
      .right {
        flex: 1;
        .title {
          font-size: 26rpx;
          color: #666;
        }
        .value {
          font-size: 30rpx;
          color: #333;
        }
      }
    }
  }
}
.myMap {
  height: 10px;
}
