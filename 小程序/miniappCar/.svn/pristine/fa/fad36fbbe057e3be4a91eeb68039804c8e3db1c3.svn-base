/**
 * api列表
 */
import request from "./request";

const baseUrl = "http://192.168.1.94:9020/portal-car";

// 绑定
export async function reqWeixinAdd(params) {
  return request({
    url: `${baseUrl}/api/wppersonweixinadd`,
    data: params,
    method: "POST"
  });
}
// 登录
export async function reqLogin(params) {
  return request(
    {
      url: `${baseUrl}/api/login?openID=${params.openID}&weixin=${
        params.weixin
      }`,
      // data: params,
      method: "POST"
    },
    true
  );
}
//查询车辆最新状态接口
export async function reqCarStateList(params) {
  return request({
    url: `${baseUrl}/api/gpsdevicebuffers/getCarLastStateBySimNo?userId=${
      params.UserId
    }&mapType=3`,
    method: "GET"
  });
}
