<block>
    <view class="map_container">
        <map id="myMap" longitude="{{longitude}}" latitude="{{latitude}}" include-points="{{carStateList}}"
        markers="{{carStateList}}" bindmarkertap="handleMakertap" style="width: 100%; height: 100%;">
            <cover-view class="marker-info" __triggerObserer="{{ _triggerObserer }}"
            style="{{anonymousState__temp}}">
                <cover-view class="center" __triggerObserer="{{ _triggerObserer }}">
                    <cover-view class="item" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="left" __triggerObserer="{{ _triggerObserer }}">
                            <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">车牌号</cover-view>
                            <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">粤ABX028</cover-view>
                        </cover-view>
                        <cover-view class="right" __triggerObserer="{{ _triggerObserer }}">
                            <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">类型</cover-view>
                            <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">洒水车</cover-view>
                        </cover-view>
                    </cover-view>
                    <cover-view class="item" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="left" __triggerObserer="{{ _triggerObserer }}">
                            <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">速度</cover-view>
                            <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">0.1 km/h</cover-view>
                        </cover-view>
                        <cover-view class="right" __triggerObserer="{{ _triggerObserer }}">
                            <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">熄火</cover-view>
                            <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">3h 48min</cover-view>
                        </cover-view>
                    </cover-view>
                    <cover-view class="item" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="left" __triggerObserer="{{ _triggerObserer }}">
                            <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">司机</cover-view>
                            <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">张晓斌</cover-view>
                        </cover-view>
                        <cover-view class="right" __triggerObserer="{{ _triggerObserer }}">
                            <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">电话</cover-view>
                            <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">13566789879</cover-view>
                        </cover-view>
                    </cover-view>
                    <cover-view class="item" __triggerObserer="{{ _triggerObserer }}">
                        <cover-view class="left" __triggerObserer="{{ _triggerObserer }}">
                            <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">车牌号</cover-view>
                            <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">粤ABX028</cover-view>
                        </cover-view>
                        <cover-view class="right" __triggerObserer="{{ _triggerObserer }}">
                            <cover-view class="title" __triggerObserer="{{ _triggerObserer }}">标题</cover-view>
                            <cover-view class="value" __triggerObserer="{{ _triggerObserer }}">值</cover-view>
                        </cover-view>
                    </cover-view>
                </cover-view>
                <cover-view className="footer" __triggerObserer="{{ _triggerObserer }}">底部</cover-view>
            </cover-view>
        </map>
        <view class="reload-btn" bindtap="handleReloadBtn"></view>
    </view>
</block>